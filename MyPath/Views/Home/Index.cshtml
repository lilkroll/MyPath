@model IEnumerable<Post> 
@if (Model is null)
            {
<p>No Posts Exists</p>
            }
@if (Model.Count() > 0)
{
                @foreach (var obj in Model)
                {
                    <h2>@obj.Header</h2>                   
                    <p>@obj.Content</p>
                    <p>@obj.Date</p>

                }
}
else
{
    <p>No Posts Exists</p>
}